// laregst number in array
#include<bits/stdc++.h>
using namespace std;
 
int sortArr(vector<int>& arr) {
    // brute force
    // TC : O(nlogn)
    // SC : O(logn)
    // sort(arr.begin(),arr.end());
    // return arr[arr.size()-1];
    // iterative
    // TC : O(n)
    // SC : O(1)
    int max = arr[0];
    int n=arr.size();
    for (int i = 0; i < n; i++) {
        if (max < arr[i]) {
        max = arr[i];
        }
    }
    return max;
}
 


